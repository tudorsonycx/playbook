# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /api     controllers.ApplicationController.index

GET     /create  controllers.ApplicationController.create

GET     /read/:id     controllers.ApplicationController.read(id: String)

GET     /update/:id   controllers.ApplicationController.update(id: String)

GET     /delete/:id   controllers.ApplicationController.delete(id: String)